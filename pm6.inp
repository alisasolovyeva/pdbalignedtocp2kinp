&GLOBAL
  PROJECT pdbid
  RUN_TYPE GEO_OPT
  PRINT_LEVEL MEDIUM
  EXTENDED_FFT_LENGTHS
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &SUBSYS
    &CELL
      A     55.00000000    0.000000000    0.000000000
      B     0.000000000    55.00000000    0.000000000
      C     0.000000000    0.000000000    55.00000000
      PERIODIC NONE
    &END CELL
    &COORD
    &END COORD
  &END SUBSYS
  &DFT
    &SCCS
      ALPHA -22.0
      BETA -0.35
      DELTA_RHO 2.0E-5
      DERIVATIVE_METHOD FFT
      DIELECTRIC_CONSTANT 78.36
      EPS_SCCS 1.0E-6
      GAMMA 72.0
      EPS_SCF 5.00000E-001
      MAX_ITER 500
      METHOD Andreussi
      MIXING 0.6
      &ANDREUSSI
        RHO_MAX 0.0050
        RHO_MIN 0.0001
      &END ANDREUSSI
    &END SCCS
    CHARGE
    &QS
      METHOD PM6
      &SE
        PERIODIC NONE
        DISPERSION
        DISPERSION_PARAMETER_FILE dftd3.dat
      &END SE
    &END QS
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.0E-7
      MAX_SCF 1000
    &END SCF
  &END DFT
  &PRINT
    &FORCES ON
    &END FORCES
  &END PRINT
&END FORCE_EVAL
&MOTION
  &GEO_OPT
    TYPE MINIMIZATION
    MAX_DR    1.0E-03
    MAX_FORCE 1.0E-03
    RMS_DR    1.0E-03
    RMS_FORCE 1.0E-03
  &END GEO_OPT
  &CONSTRAINT
    &FIXED_ATOMS
      COMPONENTS_TO_FIX XYZ
      LIST 1..427
    &END FIXED_ATOMS
  &END CONSTRAINT
&END MOTION
